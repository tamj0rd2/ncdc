{
  "name": "ncdc",
  "version": "0.0.13",
  "description": "",
  "main": "dist/index.js",
  "bin": {
    "ncdc": "./bin/ncdc"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "typecheck": "tsc",
    "typecheck:watch": "tsc --watch",
    "compile": "babel src --out-dir dist --extensions .ts --ignore 'src/**/*.spec.ts','src/**/*.d.ts' --source-maps inline",
    "build": "run-s clean typecheck compile",
    "prepublishOnly": "npm t && git push",
    "prepare": "npm run build",
    "test": "jest --collectCoverage",
    "lint": "eslint '**/*.ts' --color --ignore-path .gitignore --cache"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint -- --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tamj0rd2/ncdc.git"
  },
  "author": "Tamara Jordan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tamj0rd2/ncdc/issues"
  },
  "homepage": "https://github.com/tamj0rd2/ncdc#readme",
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@types/ajv": "^1.0.0",
    "@types/express": "^4.17.2",
    "@types/jest": "^25.2.1",
    "@types/js-yaml": "^3.12.1",
    "@types/node": "^13.11.1",
    "@types/supertest": "^2.0.8",
    "@types/yup": "^0.26.26",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "babel-jest": "^25.3.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-jest": "^23.0.4",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^4.0.10",
    "jest": "^25.3.0",
    "lint-staged": "^10.1.2",
    "nodemon": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "source-map-support": "^0.5.16",
    "supertest": "^4.0.2",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "ajv": "^6.10.2",
    "axios": "^0.19.0",
    "chalk": "^4.0.0",
    "escape-string-regexp": "^3.0.0",
    "express": "^4.17.1",
    "js-yaml": "^3.13.1",
    "typescript-json-schema": "^0.42.0",
    "winston": "^3.2.1",
    "yargs": "^15.3.1",
    "yup": "^0.28.3"
  }
}
